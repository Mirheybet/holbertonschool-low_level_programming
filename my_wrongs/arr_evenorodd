#include <stdio.h>

int fact(int* ptr);


int main()
{
    int n[5]= {1,2,3,4,5};
    
    for (int i =0 ; i < 5 ; i++)
    {
        if(n[i] % 2 == 0)
        {
            printf ("This element <<< %d >>> is  even ! \n", n[i]);
        }
        else 
            printf ("This element <<< %d >>> is  odd ! \n", n[i]);
            
    }
}

#include <stdio.h>

int main() 
{
    int arr[7]={0,1,2,3,4,5,6};
    int max , min ; 
    max , min = arr[0];
    
    for (int i = 0 ; i < 7 ; i++)
    {
        if(arr[i]<min)
        {
            min=arr[i];
        }
        
        if (arr[i]>max)
        {
            max=arr[i];
        }
    }
    
    
    
    printf("MAX = %d\n",max);
    printf("MIN = %d\n",min);
    
    
}

#include <stdio.h>

int main()
{
    int size;
    printf("Write ARRAY size => ");
    scanf("%d",&size);
    
    int n[size];
    float sum = 0 ;
    
    for(int i = 0 ; i< size ; i++)
    { 
        printf("Inlude elements of array => ") ;
        scanf("%d", &n[i]);
        sum+=n[i] ;
    }
    printf("Result of Average => %.4f\n",sum/size );
    return 0 ; 
}
